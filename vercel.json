{
    "version": 2,
    "builds": [
      {
        "src": "api/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/app.js"
      },
      {
        "src": "/assets/(.*)",
        "dest": "dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dist/index.html"
      }
    ]
  }